{
  "expo": {
    "name": "rimmind",
    "slug": "rimmind",
    "scheme": "rimmind",
    "version": "1.2.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSAppTransportSecurity": {
          "allowsArbitraryLoads": true
        }
      },
      "runtimeVersion": {
        "policy": "appVersion"
      }
    },
    "android": {
      "permissions": [
        "INTERNET"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.android.rimmind",
      "runtimeVersion": "1.0.0",
      "versionCode": 3
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro",
      "clientId": "50096351635-0vu6ql2llffp5ldpl4fv82heoshmf6c1.apps.googleusercontent.com",
      "config": {
        "googleSignIn": {
          "webClientId": "50096351635-0vu6ql2llffp5ldpl4fv82heoshmf6c1.apps.googleusercontent.com"
        }
      }
    },
    "plugins": [
      "@react-native-google-signin/google-signin",
      "expo-router",
      [
        "expo-updates",
        {
          "username": "shivaradhan"
        }
      ]
    ],
    "extra": {
